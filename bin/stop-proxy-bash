#!/bin/bash
cd "$(dirname "$0")/.."
if [ -f logs/proxy.pid ]; then
    pid=$(cat logs/proxy.pid)
    if kill $pid 2>/dev/null; then
        echo "✅ Proxy stopped (PID: $pid)"
    else
        echo "⚠️  Process $pid not found"
    fi
    rm -f logs/proxy.pid
else
    echo "❌ No proxy PID file found"
fi